function rungcc() {
	echo -e '\n\ntesting' $3 'with GCC' $2 '...\n\n'
	if g++-$2 -std=c++$1 -Wall -Wextra -I../.. $3 -o a.out
	then
		./a.out
		rm a.out
	fi
}

function runclang() {
	echo -e '\n\ntesting' $3 'with Clang' $2 '...\n\n'
	if clang++-$2 -std=c++$1 -Wall -Wextra -I../.. $3 -o a.out
	then
		./a.out
		rm a.out
	fi
}
 
